{
    "name": "j-jalving/statamic-autograph",
    "description": "E-mail signature <HTML> code generator for Statamic",
    "keywords": [
        "statamic",
        "email",
        "signature",
        "html"
    ],    
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "JJalving\\Autograph\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Autograph",
            "description": "Autograph addon"
        },
        "laravel": {
            "providers": [
                "JJalving\\Autograph\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
